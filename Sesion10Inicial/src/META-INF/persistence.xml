<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="DAWII-Sesion10">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>edu.cibertec.jpa.Articulo</class>
		<class>edu.cibertec.jpa.Cliente</class>
		<class>edu.cibertec.jpa.FacCabe</class>
		<class>edu.cibertec.jpa.FacDeta</class>
		<class>edu.cibertec.jpa.Marca</class>
		<class>edu.cibertec.jpa.Vendedor</class>
		<properties>
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/cibertec2013" />
			<property name="eclipselink.jdbc.user" value="root" />
			<property name="eclipselink.jdbc.password" value="mysql" />
		</properties>
	</persistence-unit>
</persistence>
